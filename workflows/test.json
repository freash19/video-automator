{
  "name": "select_parts_and_move",
  "steps": [
    {
      "id": "1",
      "type": "navigate",
      "params": {
        "url": "https://app.heygen.com/projects",
        "wait_until": "domcontentloaded",
        "timeout_ms": 120000
      }
    },
    {
      "id": "2",
      "type": "wait_for",
      "params": {
        "selector": "text={{episode_id}}",
        "state": "visible",
        "timeout_ms": 60000
      }
    },
    {
      "id": "3",
      "type": "select_episode_parts",
      "params": {
        "episode": "{{episode_id}}",
        "title_selector": "text={{episode_id}}",
        "checkbox_selector": "role=checkbox",
        "button_selector": "",
        "timeout_ms": 60000,
        "hover_sec": 0.15,
        "card_xpath": "xpath=ancestor::div[1]"
      }
    }
  ],
  "settings": {}
}
